<?xml version="1.0" encoding="utf-8"?><TableLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/jokerbckg" >    <TableRow        android:id="@+id/tableRow1"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:baselineAligned="false" >        <ImageButton            android:id="@+id/chuckNorris"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/chuckNorris"            android:scaleType="fitStart"            android:src="@drawable/chuck" />        <ImageButton            android:id="@+id/ozwierzetach"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/oZwierzetach"            android:scaleType="fitStart"            android:src="@drawable/ozwierzetach" />        <ImageButton            android:id="@+id/turbosuchary"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/turboSuchary"            android:scaleType="fitStart"            android:src="@drawable/turbosuchary" />    </TableRow>    <TableRow        android:id="@+id/tableRow2"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >        <ImageButton            android:id="@+id/chamskie"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/chamskie"            android:scaleType="fitStart"            android:src="@drawable/chamskie" />        <ImageButton            android:id="@+id/ostudentach"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/oStudentach"            android:scaleType="fitStart"            android:src="@drawable/ostudentach" />        <ImageButton            android:id="@+id/ofacetach"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:adjustViewBounds="true"            android:background="@null"            android:contentDescription="@string/oFacetach"            android:scaleType="fitStart"            android:src="@drawable/ofacetach" />    </TableRow>    <TableRow        android:id="@+id/tableRow4"        android:layout_width="wrap_content"        android:layout_height="fill_parent" >        <ImageButton            android:id="@+id/oBlondynkach"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/oBlondynkach"            android:scaleType="fitStart"            android:src="@drawable/button1" />    </TableRow>    <TableRow        android:id="@+id/tableRow5"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow>    <TableRow        android:id="@+id/tableRow3"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow></TableLayout>