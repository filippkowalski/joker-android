<?xml version="1.0" encoding="utf-8"?><TableLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/jokerbckg" >    <TableRow        android:id="@+id/tableRow1"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >        <ImageButton                android:id="@+id/oBlondynkach"                android:layout_width="0dip"                android:layout_height="match_parent"                android:src="@drawable/button1"                android:layout_margin="10dp"                android:layout_weight="1"                android:contentDescription="@string/oBlondynkach"                android:background="@null"                android:scaleType="fitStart" />        <ImageButton            android:id="@+id/chuckNorris"            android:layout_width="0dip"            android:layout_height="match_parent"            android:layout_margin="10dp"            android:background="@null"            android:layout_weight="1"            android:contentDescription="@string/chuckNorris"            android:src="@drawable/chuck"            android:scaleType="fitStart" />    </TableRow>    <TableRow        android:id="@+id/tableRow2"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow>    <TableRow        android:id="@+id/tableRow3"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow>    <TableRow        android:id="@+id/tableRow4"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow>    <TableRow        android:id="@+id/tableRow5"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >    </TableRow></TableLayout>