<?xml version="1.0" encoding="utf-8"?><TableLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:layout_gravity="top"    android:background="@drawable/jokerbckg" >    <TableRow        android:id="@+id/tableRow1"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_weight="2" >        <ImageButton            android:id="@+id/chamskie"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/chamskie"            android:scaleType="fitCenter"            android:src="@drawable/chamskie" />                <ImageButton            android:id="@+id/turboSuchary"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/turboSuchary"            android:scaleType="fitCenter"            android:src="@drawable/turbosuchary" />                <ImageButton            android:id="@+id/chuckNorris"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/chuckNorris"            android:scaleType="fitCenter"            android:src="@drawable/chuck" />    </TableRow>    <TableRow        android:id="@+id/tableRow2"        android:layout_width="wrap_content"        android:layout_height="wrap_content" >        <ImageButton            android:id="@+id/ofacetach"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/oFacetach"            android:scaleType="fitStart"            android:src="@drawable/ofacetach" />        <ImageButton            android:id="@+id/ozwierzetach"            android:layout_width="wrap_content"            android:layout_height="fill_parent"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/oZwierzetach"            android:scaleType="fitStart"            android:src="@drawable/ozwierzetach" />        <ImageButton            android:id="@+id/ostudentach"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/oStudentach"            android:scaleType="fitStart"            android:src="@drawable/ostudentach" />    </TableRow>    <TableRow        android:id="@+id/tableRow3"        android:layout_width="wrap_content"        android:layout_height="0dp"        android:layout_weight="0" >                <ImageButton            android:id="@+id/ulubione"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/fav"            android:scaleType="fitCenter"            android:src="@drawable/heart" />                <ImageButton            android:id="@+id/losowe"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@null"            android:contentDescription="@string/losowe"            android:scaleType="fitCenter"            android:src="@drawable/random" />    </TableRow>    <TableRow        android:id="@+id/tableRow4"        android:layout_width="wrap_content"        android:layout_height="0dp"        android:layout_weight="1" >    </TableRow>    <TableRow        android:id="@+id/tableRow5"        android:layout_width="wrap_content"        android:layout_height="0dp"        android:layout_weight="1" >    </TableRow></TableLayout>